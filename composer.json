{
    "name": "laravelplus/global-settings",
    "description": "A global settings management package for Laravel with admin panel support.",
    "keywords": ["laravel", "settings", "global-settings", "admin"],
    "license": "MIT",
    "authors": [
        {
            "name": "LaravelPlus",
            "email": "info@laravelplus.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/http": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelPlus\\GlobalSettings\\": "src/"
        },
        "classmap": [
            "database/seeders"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelPlus\\GlobalSettings\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelPlus\\GlobalSettings\\GlobalSettingsServiceProvider"
            ],
            "aliases": {
                "GlobalSettings": "LaravelPlus\\GlobalSettings\\Facades\\GlobalSettings"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
